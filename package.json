{
  "name": "satf",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "start": "node ./api/api.js",
    "start:dev": "nodemon ./api/api.js",
    "build": "office-addin-manifest validate ./interface_src/satf_production.xml && office-addin-manifest validate ./interface_src/satf_development.xml && custom-functions-metadata generate ./interface_src/custom_functions.ts ./interface_src/components/custom_functions_meta.json && webpack --mode production",
    "build:dev": "webpack --mode development",
    "build-dev": "webpack --mode development && echo . && echo . && echo . && echo Please use 'build:dev' instead of 'build-dev'.",
    "build:watch": "webpack --mode development --watch",
    "dev-server": "webpack serve --mode development",
    "debug": "office-addin-debugging start ./interface_src/satf_development.xml",
    "debug:desktop": "office-addin-debugging start ./interface_src/satf_local.xml desktop",
    "debug:web": "office-addin-debugging start ./interface_src/satf_development.xml web",
    "debug:local": "office-addin-debugging start ./interface_src/satf_local.xml web",
    "debug:stop": "office-addin-debugging stop ./interface_src/satf_development.xml",
    "install:certs": "office-addin-dev-certs install",
    "validate": "office-addin-manifest validate ./interface_src/satf_production.xml && office-addin-manifest validate ./interface_src/satf_development.xml",
    "metadata": "custom-functions-metadata generate ./interface_src/custom_functions.ts ./interface_src/components/custom_functions_meta.json"
  },
  "config": {
    "app-to-debug": "excel",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000
  },
  "author": "Casper Fibaek @ NIRAS",
  "license": "Unlicenced for now",
  "dependencies": {
    "@fluentui/react": "^7.146.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "fuse.js-ie11": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.7.1",
    "morgan": "^1.10.0",
    "nocache": "^2.1.0",
    "nodemon": "^2.0.5",
    "pg": "^8.4.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/custom-functions-runtime": "^1.3.2",
    "@types/leaflet": "^1.5.19",
    "@types/node": "^14.11.8",
    "@types/office-js": "^1.0.142",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "custom-functions-metadata": "^1.0.35",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.1",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "office-addin-cli": "^1.0.13",
    "office-addin-debugging": "^3.0.37",
    "office-addin-manifest": "^1.5.9",
    "office-addin-dev-certs": "^1.5.7",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.0.3",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  }
}
