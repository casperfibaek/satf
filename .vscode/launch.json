{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\app.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "name": "Excel Desktop",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "protocol": "inspector",
            "timeout": 600000,
            "trace": true,
            "preLaunchTask": "Debug",
            "postDebugTask": "Stop Debug",
            "outFiles": [ "${workspaceFolder}/interface/**/*.js" ]
        },
    ]
}